{
  "name": "koa-api-starter",
  "version": "2.1.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "paralect"
  ],
  "scripts": {
    "build": "run-s build:**",
    "build:email": "mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "prestart": "npm run build",
    "start": "node src/app.js",
    "test": "run-s test:**",
    "test:eslint": "eslint ./src",
    "test:mocha": "NODE_ENV=test mocha --timeout 20000 --recursive --exit -c -R spec 'src/**/*.spec.js'",
    "development": "NODE_ENV=development nodemon --watch src src/app.js",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "dependencies": {
    "@paralect/common-logger": "0.1.10",
    "@paralect/email-service": "0.1.0",
    "@paralect/node-mongo": "1.0.0",
    "app-module-path": "2.2.0",
    "bcrypt": "2.0.1",
    "joi": "13.4.0",
    "jsonwebtoken": "8.3.0",
    "kcors": "2.2.1",
    "koa": "2.5.1",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "4.0.0",
    "koa-jwt": "3.3.2",
    "koa-logger": "3.2.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-validate": "1.0.7",
    "lodash": "4.17.10",
    "nodemon": "1.17.5"
  },
  "devDependencies": {
    "@paralect/eslint-config": "0.2.0",
    "babel-eslint": "8.2.5",
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "faker": "4.1.0",
    "mjml": "4.1.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "supertest": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralect/koa-api-starter.git"
  }
}
